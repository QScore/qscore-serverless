{
  "name": "qscore-serverless",
  "version": "1.0.0",
  "description": "QScore Serverless Framework project",
  "main": "handler.js",
  "scripts": {
    "test": "export $(cat .env | xargs) && mocha --require ts-node/register test/unit/*.ts",
    "testServer": "export $(cat .env | xargs) && sls offline start",
    "testGraphQl": "export $(cat .env | xargs) && mocha --require ts-node/register test/graphql/*.ts",
    "generateSeed": "npx ts-node ./scripts/generateSeed.ts",
    "testMigration": "export AWS_SDK_LOAD_CONFIG=1 && npx ts-node ./scripts/migrateDb.ts",
    "startOffline": "nodemon --exec \"sls offline start\" -e \"ts,json\"",
    "watchTestsFast": "nodemon --exec \"npm run test\" -e \"ts\"",
    "watchTestsWithCoverage": "nodemon --exec \"npm run testCoverage\" -e \"ts\"",
    "testStuff": "export $(cat .env | xargs) && export AWS_SDK_LOAD_CONFIG=1 && npx ts-node ./scripts/testStuff.ts",
    "testCoverage": "nyc npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polson/qscore-serverless.git"
  },
  "author": "Continuum Mobile Software, LLC",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/polson/qscore-serverless/issues"
  },
  "homepage": "https://github.com/polson/qscore-serverless#readme",
  "dependencies": {
    "@types/chai-http": "^4.2.0",
    "apollo-server-lambda": "^2.11.0",
    "apollo-server-testing": "^2.12.0",
    "cryptr": "^6.0.2",
    "dotenv": "^8.2.0",
    "firebase": "^7.13.1",
    "firebase-admin": "^8.10.0",
    "graphql": "^14.6.0",
    "ioredis": "^4.16.2",
    "merge-graphql-schemas": "^1.7.6",
    "serverless": "^1.68.0",
    "serverless-bundle": "^1.3.2",
    "serverless-dynamodb-client": "0.0.2",
    "serverless-webpack-plugin": "^0.4.5",
    "webpack": "^4.42.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.47",
    "@types/chai": "^4.2.11",
    "@types/faker": "^4.1.11",
    "@types/ioredis": "^4.14.9",
    "@types/mocha": "^7.0.2",
    "@types/redis": "^2.8.18",
    "@types/sinon": "^9.0.0",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "apollo-server-types": "^0.3.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.0",
    "faker": "^4.1.0",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "happypack": "^5.0.1",
    "ioredis-mock": "^4.19.0",
    "json-loader": "^0.5.7",
    "mocha": "^7.1.1",
    "nyc": "^15.0.1",
    "serverless-dotenv-plugin": "^2.4.2",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-offline": "^6.1.4",
    "serverless-plugin-typescript": "^1.1.9",
    "serverless-webpack": "^5.3.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.2",
    "ts-sinon": "^1.0.25",
    "typescript": "^3.8.3",
    "webpack-cli": "^3.3.11",
    "webpack-fail-plugin": "^2.0.0",
    "webpack-node-externals": "^1.7.2"
  }
}
